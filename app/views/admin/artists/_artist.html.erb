<tr>
  <td><%= artist_counter + 1 + @offset %></td>
  <td><%= artist.name %></td>
  <td><%= artist.created_at.strftime("%m/%d/%Y") %></td>
  <td>
    <%= link_to edit_admin_artist_path(artist) do %>
      <span><i class="fa fa-edit fa-1px"></i></span>
    <% end %>
  </td>
  <td>
    <%= link_to admin_artist_path(id: artist.id, page: params[:page],
    search:params[:search]), method: :delete,
    data: {confirm: t("views.admin.users.user.your_sure")}, :remote => true do %>
      <span><i class="fa fa-times fa-1px"></i></span>
    <% end %>
  </td>
</tr>